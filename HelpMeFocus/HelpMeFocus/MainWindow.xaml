<Window x:Class="HelpMeFocus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelpMeFocus" 
        xmlns:views="clr-namespace:HelpMeFocus.Views"
        mc:Ignorable="d"
        Title="Timer" Height="300" Width="240" MinHeight="280" MinWidth="200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <views:MainView Grid.ColumnSpan="2"/>
    </Grid>
</Window>
